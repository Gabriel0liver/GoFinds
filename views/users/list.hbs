<div class="search">
    <label for="">Users:</label>
    <div>
        <input class="input-users" type="text">
        <div class="search-results"></div>
      </div>
</div>

<h1>List of users</h1>

<ul>
    {{#each users}}
        <li><a href="/users/{{this._id}}">{{this.username}}</a></li>
    {{/each}}
</ul>